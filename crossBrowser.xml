<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="ExtentReport.TestListener"></listener>
	</listeners>
	<test thread-count="5" name="CrossBrowserTest">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			
			<class name="executeClass.HomePageTestClass" />
			<class name="executeClass.DownloadPageTestClass" />
			<class name="executeClass.FeaturesPageTestClass" />
			<class name="executeClass.PawsomeCornerTestClass" />
			<class name="executeClass.ScreenshotsPageTestClass" />
			<class name="executeClass.TestimonialsPageTestClass" />
			
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="EdgeBrowserTest">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="executeClass.HomePageTestClass" />
			<class name="executeClass.DownloadPageTestClass" />
			<class name="executeClass.FeaturesPageTestClass" />
			<class name="executeClass.PawsomeCornerTestClass" />
			<class name="executeClass.ScreenshotsPageTestClass" />
			<class name="executeClass.TestimonialsPageTestClass" />
			
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
